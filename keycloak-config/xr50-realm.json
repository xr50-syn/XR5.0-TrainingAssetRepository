{
  "realm": "xr50",
  "enabled": true,
  "displayName": "XR50 Training",
  "displayNameHtml": "<b>XR50 Training</b>",
  "sslRequired": "none",
  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "permanentLockout": false,
  "maxFailureWaitSeconds": 900,
  "minimumQuickLoginWaitSeconds": 60,
  "waitIncrementSeconds": 60,
  "quickLoginCheckMilliSeconds": 1000,
  "maxDeltaTimeSeconds": 43200,
  "failureFactor": 5,
  "roles": {
    "realm": [
      {
        "name": "user",
        "description": "Regular user role"
      },
      {
        "name": "admin",
        "description": "Administrator role"
      },
      {
        "name": "tenantadmin",
        "description": "Tenant administrator role"
      },
      {
        "name": "systemadmin",
        "description": "System administrator role"
      }
    ]
  },
  "groups": [
    {
      "name": "users",
      "realmRoles": ["user"]
    },
    {
      "name": "admins",
      "realmRoles": ["admin"]
    },
    {
      "name": "tenant-admins",
      "realmRoles": ["tenantadmin"]
    }
  ],
  "users": [
    {
      "username": "testuser",
      "enabled": true,
      "email": "testuser@xr50.local",
      "emailVerified": true,
      "firstName": "Test",
      "lastName": "User",
      "attributes": {
        "tenantName": ["test_company"]
      },
      "credentials": [
        {
          "type": "password",
          "value": "testuser123",
          "temporary": false
        }
      ],
      "realmRoles": ["user"],
      "groups": ["users"]
    },
    {
      "username": "admin",
      "enabled": true,
      "email": "admin@xr50.local",
      "emailVerified": true,
      "firstName": "Admin",
      "lastName": "User",
      "attributes": {
        "tenantName": ["test_company"]
      },
      "credentials": [
        {
          "type": "password",
          "value": "admin123",
          "temporary": false
        }
      ],
      "realmRoles": ["admin", "user"],
      "groups": ["admins", "users"]
    },
    {
      "username": "tenantadmin",
      "enabled": true,
      "email": "tenantadmin@xr50.local",
      "emailVerified": true,
      "firstName": "Tenant",
      "lastName": "Admin",
      "attributes": {
        "tenantName": ["test_company"]
      },
      "credentials": [
        {
          "type": "password",
          "value": "tenantadmin123",
          "temporary": false
        }
      ],
      "realmRoles": ["tenantadmin", "user"],
      "groups": ["tenant-admins", "users"]
    }
  ],
  "clients": [
    {
      "clientId": "xr50-training-api",
      "name": "XR50 Training API",
      "description": "Backend API client for XR50 Training system",
      "enabled": true,
      "publicClient": false,
      "bearerOnly": true,
      "standardFlowEnabled": false,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "protocol": "openid-connect",
      "attributes": {
        "access.token.lifespan": "3600"
      }
    },
    {
      "clientId": "xr50-training-app",
      "name": "XR50 Training Application",
      "description": "Frontend application client for XR50 Training system",
      "enabled": true,
      "publicClient": true,
      "bearerOnly": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": false,
      "protocol": "openid-connect",
      "rootUrl": "http://localhost:5286",
      "baseUrl": "/",
      "redirectUris": [
        "http://localhost:5286/*",
        "http://localhost:3000/*",
        "http://localhost:4200/*",
        "http://localhost:8080/*"
      ],
      "webOrigins": [
        "http://localhost:5286",
        "http://localhost:3000",
        "http://localhost:4200",
        "http://localhost:8080",
        "+"
      ],
      "attributes": {
        "access.token.lifespan": "3600",
        "pkce.code.challenge.method": "S256"
      },
      "protocolMappers": [
        {
          "name": "tenant-mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "consentRequired": false,
          "config": {
            "userinfo.token.claim": "true",
            "user.attribute": "tenantName",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "tenantName",
            "jsonType.label": "String"
          }
        },
        {
          "name": "realm-roles-mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "consentRequired": false,
          "config": {
            "userinfo.token.claim": "true",
            "multivalued": "true",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "role",
            "jsonType.label": "String"
          }
        }
      ]
    },
    {
      "clientId": "xr50-swagger",
      "name": "XR50 Swagger UI",
      "description": "Swagger UI client for API testing",
      "enabled": true,
      "publicClient": true,
      "bearerOnly": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": false,
      "protocol": "openid-connect",
      "rootUrl": "http://localhost:5286",
      "redirectUris": [
        "http://localhost:5286/swagger/oauth2-redirect.html",
        "http://localhost:5286/*"
      ],
      "webOrigins": [
        "http://localhost:5286",
        "+"
      ],
      "attributes": {
        "access.token.lifespan": "3600",
        "pkce.code.challenge.method": "S256"
      },
      "protocolMappers": [
        {
          "name": "tenant-mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "consentRequired": false,
          "config": {
            "userinfo.token.claim": "true",
            "user.attribute": "tenantName",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "tenantName",
            "jsonType.label": "String"
          }
        },
        {
          "name": "realm-roles-mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "consentRequired": false,
          "config": {
            "userinfo.token.claim": "true",
            "multivalued": "true",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "role",
            "jsonType.label": "String"
          }
        }
      ]
    }
  ],
  "clientScopes": [
    {
      "name": "xr50-scope",
      "description": "XR50 Training API scope",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true",
        "display.on.consent.screen": "true"
      },
      "protocolMappers": [
        {
          "name": "audience-mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "consentRequired": false,
          "config": {
            "included.client.audience": "xr50-training-api",
            "id.token.claim": "false",
            "access.token.claim": "true"
          }
        }
      ]
    }
  ],
  "defaultDefaultClientScopes": [
    "profile",
    "email",
    "roles",
    "web-origins",
    "xr50-scope"
  ]
}
